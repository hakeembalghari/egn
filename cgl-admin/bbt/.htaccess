RewriteEngine On
RewriteBase /beta/cgl-admin/bbt/
RewriteRule ^([a-zA-Z0-9:_-]+)/$ index.php?p=$1 [L,QSA]
RewriteRule ^([a-zA-Z0-9:_-]+)$ index.php?p=$1 [L,QSA]
RewriteRule ^([a-zA-Z0-9:_-]+)/([a-zA-Z0-9:_-]+)$ index.php?p=$1&s=$2 [L,QSA]
RewriteRule ^([a-zA-Z0-9:_-]+)/([a-zA-Z0-9:_-]+)/$ index.php?p=$1&s=$2 [L,QSA]